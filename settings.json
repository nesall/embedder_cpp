    "current_api": "remote-hosted",
    "default_max_tokens": 2048,
    "default_max_tokens_name": "max_tokens",
    "default_temperature": 0.5,
    "max_chunks": 7,
    "max_context_tokens": 20000,
    "max_full_sources": 2,
    "max_related_per_source": 3,
    "timeout_ms": 120000
  },
  "logging": {
    "diagnostics_file": "embedder_diag.log",
    "level": "info",
    "log_to_file": true,
    "logging_file": "embedder.log"
  },
  "source": {
    "default_extensions": [
      ".cpp",
      ".h",
      ".py",
      ".js",
      ".md"
    ],
    "encoding": "utf-8",
    "global_exclude": [
      "*/node_modules/*",
      "*.min.js",
      "*.log",
      "*/.git/*",
      ".gitignore",
      "*/CVS/*",
      "*/debug/*",
      "*/release/*",
      "*/lib/*",
      "*/docker/*",
      "*/fonts/*",
      "*/images/*",
      "*/test/*",
      "*/tests/*",
      "*/example/*",
      "*/examples/*",
      "*/obj/*",
      "*/build_dbg/*",
      "*/build_rel/*"
    ],
    "max_file_size_mb": 10,
    "paths": [
      {
        "exclude": [
          "*/build/*",
          "*/build_rel/*",
          "*/dist/*",
          "*/test/*",
          "*/3rdparty/*"
        ],
        "extensions": [
          ".cpp",
          ".h",
          ".hpp"
        ],
        "path": "../../embedder_cpp",
        "recursive": true,
        "type": "directory"
      },
      {
        "exclude": [
          "*/build/*",
          "*/build_rel/*",
          "*/dist/*",
          "*/test/*"
        ],
        "extensions": [
          ".cpp",
          ".h",
          ".hpp"
        ],
        "path": "../../rag-code-assistant/clients/webview",
        "recursive": true,
        "type": "directory"
      },
      {
        "exclude": [
          "*/node_modules/*",
          "*/dist/*"
        ],
        "extensions": [
          ".svelte",
          ".ts",
          ".js",
          ".html"
        ],
        "path": "../../rag-code-assistant/clients/spa-svelte2",
        "recursive": true,
        "type": "directory"
      },
      {
        "exclude": [],
        "extensions": [
          ".md",
          ".txt"
        ],
        "path": "./docs",
        "recursive": false,
        "type": "directory"
      },
      {
        "path": "./README.md",
        "type": "file"
      },
      {
        "headers": {
          "Authorization": "Bearer ${GITHUB_TOKEN}"
        },
        "type": "url",
        "url": "https://raw.githubusercontent.com/user/repo/main/docs/api.md"
      },
      {
        "timeout_ms": 10000,
        "type": "url",
        "url": "https://docs.example.com/api"
      }
    ]
  },
  "tokenizer": {
    "config_path": "../assets/bge_tokenizer.json"
  }
}