{
  "chunking": {
    "code_boundaries": [
      "function",
      "class",
      "method"
    ],
    "nof_max_tokens": 450,
    "nof_min_tokens": 50,
    "overlap_percentage": 0.1,
    "semantic": true
  },
  "database": {
    "distance_metric": "cosine",
    "distance_metric_": "l2",
    "index_path": "db_embeddings.index",
    "max_elements": 100000,
    "sqlite_path": "db_metadata.db",
    "vector_dim": 768
  },
  "embedding": {
